[
    '{{repeat(50, 80)}}',
    {
        incidences: function() { return Math.floor(Math.random() * 80);},
        disease: function () {
            var attribute = ['Artrite', 'Candidiase Oral', 'Doença Cardíaca Congénita', 'Doença da Tiroide', 'Doença Venérea', 'Enfarte Miocárdio', 'Febre Reumática', 'Gânglios aumentados de volume', 'Glaucoma', 'Osteoporose'];
            var max = attribute.length - 1;
            var min = 0;

            var ar = [];
            for (var i = min; i < max; i++) {
                ar[i] = i;
            }
            // randomize the array
            ar.sort(function () {
                return Math.random() - 0.5;
            });

            //var index = ar.pop();
            var index = Math.floor(Math.random() * (max - min)) + min;
            return attribute[index];
        },
        medication: function () {
            var attribute = ['Anti-hipertensor', 'Broncodilatador', 'Anti-depressor', 'Anti-ácidos', 'Estatinas', 'Anti-diabéticos', 'Análgésicos', 'Aspirina', 'Esteróides'];
            var max = attribute.length - 1;
            var min = 0;

            var ar = [];
            for (var i = min; i < max; i++) {
                ar[i] = i;
            }
            // randomize the array
            ar.sort(function () {
                return Math.random() - 0.5;
            });

            //var index = ar.pop();
            var index = Math.floor(Math.random() * (max - min)) + min;
            return attribute[index];
        }
    }
